{
  "name": "o1js-jal",
  "version": "0.0.1",
  "description": "o1js JAL implementation",
  "license": "(Apache-2.0 AND MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcred-org/o1js-jal.git"
  },
  "keywords": [
    "o1js-jal",
    "jal",
    "o1js JSON abstract language",
    "o1js"
  ],
  "type": "module",
  "scripts": {
    "test": "npx tsm ./node_modules/uvu/bin.js ./test",
    "build": "rm -r dist & tsc -p tsconfig.build.json",
    "publish": "npm run build && npm run publish"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "mustache": "^4.2.0",
    "o1js": "^0.15.0",
    "o1js-trgraph": "^1.0.2",
    "sort-keys": "^5.0.0"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "@types/node": "20.8.0",
    "tsm": "^2.3.0",
    "typescript": "^4.9.5",
    "uvu": "^0.5.6"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.t"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
